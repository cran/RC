\name{RC.load}
\Rdversion{1.1}
\alias{RC.load}
\title{
Load image
}
\description{
Loads an image from the repository. Unlike RC.save.image() this function is executed quickly.
}
\usage{
RC.load(url = "", uid = "", pwd = "")
}
\arguments{
  \item{url}{
The URL that uniquely identifies the image in the repository at FreeStatistics.org
}
  \item{uid}{
User ID of your account at FreeStatistics.org
}
  \item{pwd}{
Password
}
}
